# Arquivo de exemplo para vari√°veis de ambiente

# Database
DATABASE_URL=sqlite:///./portfolio.db
# Para PostgreSQL use: postgresql://user:password@localhost/portfolio

# CORS Origins - Aceita dois formatos:
# Formato JSON: CORS_ORIGINS=["http://localhost:3000","https://portfolio-frontend.mktdr8.easypanel.host","https://portfolio.ikehara.dev.br"]
# Formato CSV: CORS_ORIGINS=http://localhost:3000,https://portfolio-frontend.mktdr8.easypanel.host,https://portfolio.ikehara.dev.br
CORS_ORIGINS=["http://localhost:3000","http://localhost:5173","https://portfolio-frontend.mktdr8.easypanel.host","https://portfolio.ikehara.dev.br"]

# Security
SECRET_KEY=your-secret-key-change-in-production
DEBUG=False

# Google OAuth (get from https://console.cloud.google.com/)
GOOGLE_CLIENT_ID=your-google-client-id-here
GOOGLE_CLIENT_SECRET=your-google-client-secret-here
GOOGLE_REDIRECT_URI=https://portfolio-backend.mktdr8.easypanel.host/api/auth/google/callback

# Authorized Email (only this email can access admin)
AUTHORIZED_EMAIL=vhikehara@gmail.com
