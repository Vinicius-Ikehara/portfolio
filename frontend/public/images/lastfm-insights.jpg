<svg width="800" height="450" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1e293b;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#0f172a;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0c4a6e;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="card" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#334155;stop-opacity:0.8" />
      <stop offset="100%" style="stop-color:#1e293b;stop-opacity:0.9" />
    </linearGradient>
  </defs>

  <rect width="800" height="450" fill="url(#bg)"/>

  <!-- Grid pattern -->
  <pattern id="grid" width="40" height="40" patternUnits="userSpaceOnUse">
    <circle cx="2" cy="2" r="1" fill="rgba(255,255,255,0.1)"/>
  </pattern>
  <rect width="800" height="450" fill="url(#grid)"/>

  <!-- Music note icon -->
  <g transform="translate(80, 80)">
    <circle cx="0" cy="0" r="60" fill="rgba(56, 189, 248, 0.15)" stroke="#38bdf8" stroke-width="2"/>
    <path d="M -20 -10 L -20 20 Q -20 30 -10 30 Q 0 30 0 20 Q 0 10 -10 10 Q -15 10 -15 15 L -15 -5 L 15 -15 L 15 10 Q 15 20 25 20 Q 35 20 35 10 Q 35 0 25 0 Q 20 0 20 5 L 20 -20 L -20 -10 Z"
          fill="#38bdf8"/>
  </g>

  <!-- Bar chart -->
  <g transform="translate(250, 120)">
    <rect x="0" y="80" width="40" height="100" fill="#22c55e" rx="4"/>
    <rect x="60" y="40" width="40" height="140" fill="#38bdf8" rx="4"/>
    <rect x="120" y="60" width="40" height="120" fill="#a855f7" rx="4"/>
    <rect x="180" y="100" width="40" height="80" fill="#f59e0b" rx="4"/>
    <rect x="240" y="90" width="40" height="90" fill="#ef4444" rx="4"/>
  </g>

  <!-- Title -->
  <text x="400" y="320" font-family="Arial, sans-serif" font-size="48" font-weight="bold" fill="#ffffff" text-anchor="middle">
    Last.fm Insights
  </text>

  <!-- Subtitle -->
  <text x="400" y="360" font-family="Arial, sans-serif" font-size="20" fill="#7dd3fc" text-anchor="middle">
    Music Analytics Dashboard
  </text>

  <!-- Decorative elements -->
  <circle cx="650" cy="100" r="8" fill="#38bdf8" opacity="0.6">
    <animate attributeName="opacity" values="0.3;0.8;0.3" dur="2s" repeatCount="indefinite"/>
  </circle>
  <circle cx="680" cy="140" r="6" fill="#a855f7" opacity="0.6">
    <animate attributeName="opacity" values="0.8;0.3;0.8" dur="2s" repeatCount="indefinite"/>
  </circle>
  <circle cx="710" cy="110" r="7" fill="#22c55e" opacity="0.6">
    <animate attributeName="opacity" values="0.5;1;0.5" dur="2s" repeatCount="indefinite"/>
  </circle>

  <!-- Stats cards -->
  <g transform="translate(520, 240)">
    <rect width="200" height="60" fill="url(#card)" rx="8" stroke="#38bdf8" stroke-width="1"/>
    <text x="20" y="25" font-family="Arial, sans-serif" font-size="12" fill="#9ca3af">Top 10 Daily</text>
    <text x="20" y="45" font-family="Arial, sans-serif" font-size="18" font-weight="bold" fill="#ffffff">Tracks</text>
    <circle cx="170" cy="30" r="15" fill="rgba(56, 189, 248, 0.2)" stroke="#38bdf8" stroke-width="1"/>
    <path d="M 168 25 L 168 35 L 173 30 Z" fill="#38bdf8"/>
  </g>
</svg>
